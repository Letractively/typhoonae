upstream appcfg_service {
  server 127.0.0.1:9190;
}

server {
  listen      9193;
  server_name localhost;
  ssl on;
  ssl_certificate ${buildout:directory}/etc/ssl/appcfgd.cert;
  ssl_certificate_key ${buildout:directory}/etc/ssl/appcfgd.key;
  access_log  ${buildout:directory}/var/log/appcfgd-httpd-access.log;
  error_log   ${buildout:directory}/var/log/appcfgd-httpd-error.log;

  location / {
    proxy_pass http://appcfg_service;
    proxy_read_timeout 500;
    proxy_send_timeout 500;
  }
}